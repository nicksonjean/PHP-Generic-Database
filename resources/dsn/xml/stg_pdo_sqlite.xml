<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<engine>pdo</engine>
	<driver>sqlite</driver>
	<database>../../resources/database/sqlite/data/DB.SQLITE</database>
	<charset>utf8</charset>
	<options>
		<option name="PDO::ATTR_PERSISTENT">true</option>
		<option name="PDO::ATTR_EMULATE_PREPARES">true</option>
		<option name="PDO::ATTR_DEFAULT_FETCH_MODE">PDO::FETCH_OBJ</option>
	</options>
	<exception>true</exception>
</config>
