<?xml version="1.0" encoding="UTF-8"?>
<config>
    <engine>json</engine>
    <database>../../resources/database/json</database>
    <charset>utf8</charset>
    <options>
        <option name="JSON::ATTR_PERSISTENT">true</option>
        <option name="JSON::ATTR_AUTOCOMMIT">true</option>
        <option name="JSON::ATTR_CONNECT_TIMEOUT">28800</option>
        <option name="JSON::ATTR_DEFAULT_FETCH_MODE">JSON::FETCH_OBJ</option>
    </options>    
    <exception>true</exception>
</config>
