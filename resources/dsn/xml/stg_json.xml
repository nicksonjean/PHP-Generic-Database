<?xml version="1.0" encoding="UTF-8"?>
<config>
    <engine>json</engine>
    <database>../../resources/database/json</database>
    <charset>utf8</charset>
    <options>
        <option>
            <JSON::ATTR_PERSISTENT>true</JSON::ATTR_PERSISTENT>
            <JSON::ATTR_AUTOCOMMIT>true</JSON::ATTR_AUTOCOMMIT>
            <JSON::ATTR_CONNECT_TIMEOUT>28800</JSON::ATTR_CONNECT_TIMEOUT>
            <JSON::ATTR_DEFAULT_FETCH_MODE>JSON::FETCH_OBJ</JSON::ATTR_DEFAULT_FETCH_MODE>
        </option>
    </options>
    <exception>true</exception>
</config>
